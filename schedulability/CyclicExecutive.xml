			<label kind="synchronisation" x="-654" y="-85">release!</label>
			<label kind="assignment" x="-663" y="-68">sensors = sensor_count</label>
			<nail x="-586" y="170"/>
			<nail x="-586" y="-246"/>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id2"/>
